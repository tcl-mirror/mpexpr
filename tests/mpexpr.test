test mpexpr-4.8 {string operators} {mpexpr {"def" >= "def"}} 1
test mpexpr-4.9 {string operators} {mpexpr {"g" >= "def"}} 1
test mpexpr-4.10 {string operators} {mpexpr {"abc" <= "abd"}} 1
test mpexpr-4.11 {string operators} {mpexpr {"abd" <= "abd"}} 1
